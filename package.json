{
  "name": "discord-bot-template",
  "version": "3.0.1",
  "description": "Template professionale per Discord Bot con TypeScript e discord.js v14+",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "deploy-commands": "ts-node src/handlers/deploy-commands.ts",
    "prepare": "husky install"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "discord.js"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.5.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "husky": "^9.1.7",
    "mysql2": "^3.14.2",
    "undici": "^7.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
